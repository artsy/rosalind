{
  "name": "rosalind-webpack",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "resolutions": {
    "relay-runtime": "https://github.com/artsy/relay/releases/download/v1.5.0-artsy.3/relay-runtime-1.5.0-artsy.3.tgz"
  },
  "dependencies": {
    "@artsy/palette": "4.14.9",
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "@rails/webpacker": "4.0.2",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-styled-components": "1.10.0",
    "chrono-node": "1.3.5",
    "css-loader": "0.28.11",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "file-loader": "1.1.11",
    "jest": "22.4.4",
    "jest-styled-components": "6.3.1",
    "json-loader": "0.5.7",
    "lodash.defaults": "4.2.0",
    "lodash.intersection": "4.4.0",
    "lodash.pickby": "4.6.0",
    "lodash.zipobject": "4.1.3",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "raf": "3.4.1",
    "react": "16.6.3",
    "react-addons-test-utils": "15.6.2",
    "react-autosuggest": "9.4.2",
    "react-dom": "16.6.3",
    "react-test-renderer": "16.6.3",
    "stats-webpack-plugin": "0.6.2",
    "style-loader": "0.21.0",
    "styled-components": "4.2.0",
    "webpack-cli": "3.3.2",
    "whatwg-fetch": "2.0.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/webpack/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/webpack/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "/config/",
      "/node_modules/",
      "/vendor/bundle/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/app/javascript/test/setup.js"
  },
  "scripts": {
    "lint": "eslint app/javascript --ext .js,.jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose",
    "storybook": "./script/check_for_server.rb && start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "standard": {
    "globals": [
      "beforeEach",
      "beforeAll",
      "describe",
      "expect",
      "it",
      "test",
      "jest"
    ]
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "22.4.4",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.2.0",
    "lint-staged": "8.1.6",
    "prettier": "1.17.0",
    "webpack-dev-server": "3.4.1",
    "@storybook/react": "5.0.11",
    "@storybook/addon-actions": "5.0.11",
    "@storybook/addon-links": "5.0.11",
    "@storybook/addons": "5.0.11"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/javascript/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.rb": [
      "bundle exec rubocop -a",
      "git add"
    ]
  }
}
