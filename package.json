{
  "name": "rosalind-webpack",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "resolutions": {
    "relay-runtime": "https://github.com/alloy/relay/releases/download/v1.5.0-artsy.3/relay-runtime-1.5.0-artsy.3.tgz"
  },
  "dependencies": {
    "@artsy/palette": "^4.11.4",
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.47",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.47",
    "@babel/polyfill": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-react": "^7.0.0-beta.47",
    "@rails/webpacker": "3.5",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-module-resolver": "^3.1",
    "babel-plugin-styled-components": "^1.5.1",
    "chrono-node": "^1.3.5",
    "css-loader": "^0.28.9",
    "enzyme": "^3",
    "enzyme-adapter-react-16": "^1.7.0",
    "file-loader": "^1",
    "jest": "^22",
    "jest-styled-components": "^5",
    "json-loader": "^0.5.7",
    "lodash.defaults": "^4.2.0",
    "lodash.intersection": "^4.4.0",
    "lodash.pickby": "^4.6.0",
    "lodash.zipobject": "^4.1.3",
    "moment": "^2",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.4.2",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.2",
    "stats-webpack-plugin": "^0.6",
    "style-loader": "^0.21",
    "styled-components": "^3",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/webpack/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/webpack/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "/config/",
      "/node_modules/",
      "/vendor/bundle/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/app/javascript/test/setup.js"
  },
  "scripts": {
    "lint": "eslint app/javascript --ext .js,.jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose"
  },
  "standard": {
    "globals": [
      "beforeEach",
      "beforeAll",
      "describe",
      "expect",
      "it",
      "test",
      "jest"
    ]
  },
  "devDependencies": {
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.4.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "1.17.0",
    "webpack-dev-server": "^2.11"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/javascript/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.rb": [
      "bundle exec rubocop -a",
      "git add"
    ]
  }
}
