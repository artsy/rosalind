    "webpack-dev-server": "^1.9.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {},
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/webpack/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/webpack/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/vendor/bundle/"
    ]
  },
  "scripts": {
    "lint": "standard $(find config webpack -name *.js)",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose"
  },
  "standard": {
    "globals": [
      "beforeEach",
      "describe",
      "expect",
      "it",
      "jest"
    ]
  }
}
